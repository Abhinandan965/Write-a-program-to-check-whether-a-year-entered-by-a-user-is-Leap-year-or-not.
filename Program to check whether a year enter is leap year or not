#include <iostream>
using namespace std ;
int main ( ) 
{
int year;

cout<<"Enter year to check if its leap year or not"<<end1;

cin>>year;

if(year%4==0)

{

    if(year%100==0)
    {
        if(year %400==0)
         {
          cout<<"Leap year<<end1;
         }

         else
         {
          cout<<"Not a Leap year"<<end1;
         }
    }
    else
    {
     cout<<"Not a Leap year"<end1;
    }
}
else
{
cout<<" Not a Leap year "<<end1; 
